#define COMPUTE_ENTRY MainCS

#program compute

#include <TDEngine2MathUtils.inc>


RWTexture3D<float4> noiseTexture;


static const float InvNoiseTexSize = 0.03125;


[numthreads(1, 1, 1)]
void MainCS(uint3 id : SV_DispatchThreadID)
{
	float3 p = (2.0 * InvNoiseTexSize) * float3(id.x, id.y, id.z) - 1.0;
	noiseTexture[id.xyz] = float4(CalcWorleyFbmNoise3D(p, 3, 2.0), CalcWorleyFbmNoise3D(p, 3, 4.0), CalcWorleyFbmNoise3D(p, 3, 8.0), 1.0);
}

#endprogram
